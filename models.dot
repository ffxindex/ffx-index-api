digraph models_diagram {
	graph[overlap=false, splines=true, bgcolor="none"]
	"Ability" [shape=Mrecord, label="{Ability|item_id :integer\lname :character varying\ltype :character varying\lid :integer\litem_amount :integer\lcreated_at :timestamp without time zone\lupdated_at :timestamp without time zone\l}"]
	"ApplicationRecord" []
	"BribeDrop" [shape=Mrecord, label="{BribeDrop|amount :integer\lmonster_id :integer\litem_id :integer\lid :integer\lcost :integer\lcreated_at :timestamp without time zone\lupdated_at :timestamp without time zone\l}"]
	"Item" [shape=Mrecord, label="{Item|id :integer\lname :character varying\leffect :text\lcreated_at :timestamp without time zone\lupdated_at :timestamp without time zone\l}"]
	"KillDrop" [shape=Mrecord, label="{KillDrop|amount :integer\lmonster_id :integer\litem_id :integer\lid :integer\lrare :boolean\lcreated_at :timestamp without time zone\lupdated_at :timestamp without time zone\l}"]
	"Location" [shape=Mrecord, label="{Location|id :integer\lname :character varying\lcreated_at :timestamp without time zone\lupdated_at :timestamp without time zone\l}"]
	"Monster" [shape=Mrecord, label="{Monster|id :integer\lname :character varying\lcreated_at :timestamp without time zone\lupdated_at :timestamp without time zone\l}"]
	"StealDrop" [shape=Mrecord, label="{StealDrop|amount :integer\lmonster_id :integer\litem_id :integer\lid :integer\lrare :boolean\lcreated_at :timestamp without time zone\lupdated_at :timestamp without time zone\l}"]
	"Item" -> "Ability" [arrowtail=odot, arrowhead=crow, dir=both color="#42F282"]
	"Item" -> "KillDrop" [arrowtail=odot, arrowhead=crow, dir=both color="#3F3477"]
	"Item" -> "StealDrop" [arrowtail=odot, arrowhead=crow, dir=both color="#45F2EC"]
	"Item" -> "BribeDrop" [arrowtail=odot, arrowhead=crow, dir=both color="#2EA76C"]
	"Location" -> "Monster" [arrowtail=crow, arrowhead=crow, dir=both color="#A6D03A"]
	"Monster" -> "KillDrop" [arrowtail=odot, arrowhead=crow, dir=both color="#DB06F2"]
	"Monster" -> "StealDrop" [arrowtail=odot, arrowhead=crow, dir=both color="#384883"]
	"Monster" -> "BribeDrop" [arrowtail=odot, arrowhead=dot, dir=both color="#5A34A5"]
}
