	"Location" -> "Monster" [arrowtail=odot, arrowhead=crow, dir=both color="#BFA966"]
	"Monster" -> "KillDrop" [arrowtail=odot, arrowhead=crow, dir=both color="#8DAE7B"]
	"Monster" -> "StealDrop" [arrowtail=odot, arrowhead=crow, dir=both color="#025E8D"]
